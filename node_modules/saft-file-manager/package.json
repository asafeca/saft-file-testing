{
  "name": "saft-file-manager",
  "version": "1.0.2",
  "description": "This a lib to manage any saft-file",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:asafeca/saft-file-checker.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"dist/**/*.js\" \"dist/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build-all": "npm run lint && npm run format"
  },
  "keywords": [
    "saft-file"
  ],
  "author": "Adelino Safeca",
  "license": "ISC",
  "dependencies": {
    "fast-xml-parser": "^4.0.11",
    "file-type": "^17.1.1",
    "path": "^0.12.7",
    "xml2js": "^0.4.23",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/object-mapper": "^6.2.0",
    "@types/xml2js": "^0.4.11",
    "@types/xmldom": "^0.1.31",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  }
}
